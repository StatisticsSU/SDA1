\begin_deeper
\begin_layout Itemize
Nollhypotes, 
\begin_inset Formula $H_{0}$
\end_inset

: räknedata i en tabell med 
\begin_inset Formula $N$
\end_inset

 celler följer en viss specificerad fördelning med sannolikhet 
\begin_inset Formula $p_{k}$
\end_inset

 i cell 
\begin_inset Formula $k$
\end_inset

.
\end_layout

\begin_layout Itemize
Totalt antal i hela tabellen 
\begin_inset Formula $n$
\end_inset


\end_layout

\begin_layout Itemize
Förväntat antal i cell 
\begin_inset Formula $k$
\end_inset

:
\begin_inset Formula $\mathrm{Exp}_{k}=n\cdot p_{k}$
\end_inset

.
\end_layout

\begin_layout Itemize
Observerat antal i cell 
\begin_inset Formula $k$
\end_inset

: 
\begin_inset Formula $\mathrm{Obs}_{k}$
\end_inset

.
\end_layout

\begin_layout Itemize
Chi2 (
\begin_inset Formula $\chi^{2}$
\end_inset

) test för tabell med 
\begin_inset Formula $N$
\end_inset

 celler - 
\series bold
\color blue
teststatistika
\series default
\color inherit

\begin_inset Formula 
\[
\chi^{2}=\sum_{k=1}^{K}\frac{(\mathrm{Obs}_{k}-\mathrm{Exp}_{k})^{2}}{\mathrm{Exp}_{k}}=\sum_{\text{all cells}}\frac{(\mathrm{Obs}-\mathrm{Exp})^{2}}{\mathrm{Exp}}
\]

\end_inset


\end_layout

\begin_layout Itemize
Fördelning under 
\begin_inset Formula $H_{0}$
\end_inset

 - Chi2 fördelning med 
\begin_inset Formula $K-1$
\end_inset

 frihetsgrader
\begin_inset Formula 
\[
\chi^{2}\sim\chi_{K-1}^{2}
\]

\end_inset


\end_layout

\end_deeper
